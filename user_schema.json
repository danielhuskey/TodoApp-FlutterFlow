{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ToDoApp",
  "type": "object",
  "properties": {
    "users": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "birthday": {
            "type": "string",
            "format": "date-time"
          },
          "created_time": {
            "type": "string",
            "format": "date-time"
          },
          "display_name": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          },
          "photo_url": {
            "type": "string",
            "format": "uri"
          },
          "uid": {
            "type": "string"
          }
        },
        "required": ["birthday", "created_time", "display_name", "email", "photo_url", "uid"]
      }
    }
  }
}